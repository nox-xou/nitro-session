{
  "name": "@noxnox/nitro-session",
  "version": "1.3.3-0",
  "description": "Easy-to-use nitro session.",
  "license": "MIT",
  "author": "kiki-kanri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nox-xou/nitro-session.git"
  },
  "type": "module",
  "scripts": {
    "build": "ts-project-builder ./src/index.ts ./src/runtime/utils.ts -f esm --clean --preserve-modules",
    "build:playground": "nitropack build",
    "bumplog": "changelogen --bump",
    "dev": "nitro dev"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "consola": "^3.2.3",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.7",
    "node-ciphers": "1.0.2",
    "on-change": "^5.0.1",
    "unstorage": "^1.12.0"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "@types/lodash-es": "^4.17.12",
    "changelogen": "^0.5.5",
    "nitropack": "^2.9.7",
    "ts-project-builder": "3.0.1",
    "tslib": "^2.7.0"
  },
  "keywords": [
    "nitro",
    "nitro session",
    "nitropack",
    "nitropack session",
    "session"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./runtime/*": {
      "import": "./dist/runtime/*.mjs",
      "types": "./dist/runtime/*.d.ts"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts"
    }
  },
  "prettier": "@kikiutils/prettierrc",
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
